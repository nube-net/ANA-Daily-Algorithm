for _ in range(int(input())):
  x1,y1,r1, x2,y2,r2 = map(int,input().split())
  d = ((x1-x2)**2 + (y1-y2)**2)**0.5
  r1, r2 = max(r1,r2), min(r1,r2)
  if x1==x2 and y1==y2 and r1==r2: # 겹치는 경우
    print(-1)
  elif 0<r1+r2<d or r1>d+r2:
    print(0)
  elif r1+r2==d or r1==d+r2:
    print(1)
  elif r1+r2 > d:
    print(2)
