def d(n,limit):
  tmp = n
  for x in str(n):
    tmp += int(x)
  if tmp >= limit or tmp in nonself:
    return tmp
  else:
    nonself.append(tmp)
    return d(tmp,limit)

nonself = []
n = 10000
for i in range(1,n):
  if i not in nonself:
    d(i,n)
for i in range(1,n):
  if i not in nonself:
    print(i)
