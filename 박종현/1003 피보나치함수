memo = {-1:1, 0:0, 1:1}
def F(n):
    if n==0:
        return 0

    elif n==1:
        return 1

    if n in memo:
        return memo[n]
    else:
        memo[n] = F(n-1) + F(n-2)
        return memo[n]
F(40)

tc = int(input())
for i in range(tc):
    n = int(input())
    print(memo[n-1], memo[n])
