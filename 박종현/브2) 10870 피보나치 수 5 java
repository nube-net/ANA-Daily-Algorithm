import java.util.ArrayList;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		ArrayList<Integer> F = new ArrayList();
		F.add(0);
		F.add(1);
		
		for (int i=0; i<n-1; i++) {
			F.add(F.get(i)+F.get(i+1));
		}
		System.out.println(F.get(n));
	}
}
