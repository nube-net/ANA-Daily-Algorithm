for _ in range(int(input())):
  arr_len = int(input())
  arr = list(map(int,input().split()))

  subarray = [0]*(arr_len+1)
  for i in range(1,arr_len+1):
    subarray[i] = subarray[i-1] + arr[i-1]

  max = -(10**9)
  for i in range(arr_len+1):
    a = subarray[i]
    for k in range(i+1,arr_len+1):
      b = subarray[k]
      if max < b-a:
        max = b-a
  print(max)
