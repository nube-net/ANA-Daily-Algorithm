route =((2,1),(2,-1),(-2,1),(-2,-1),(1,2),(1,-2),(-1,2),(-1,-2))
c = []
b = 0
start = input()
c.append(start)
for i in range(35):
    a = input()
    if a in c:
        b += 1
    c.append(a)
    move = (ord(c[i][0]) - ord(c[i+1][0]),int(c[i][1]) - int(c[i+1][1]))
    if move not in route:
        b += 1
move = (ord(c[-1][0]) - ord(c[0][0]), int(c[-1][1]) - int(c[0][1]))
if move not in route:
    b += 1
if b == 0:
    print("Valid")
else:
    print("Invalid")
