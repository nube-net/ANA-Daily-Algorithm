b = 1
while True:
    N = int(input())
    if N == 0:
        break
    a = [[] for i in range(N)]
    for i in range(N):
        a[i] = list(input().split())
    print("Group",b)
    c = 0
    for i in range(N):
        for j in range(1,N):
            if a[i][j] == "N":
                c += 1
                print(f"{a[i-j][0]} was nasty about {a[i][0]}")
    if c == 0:
        print("Nobody was nasty")
    print("")
    b += 1
